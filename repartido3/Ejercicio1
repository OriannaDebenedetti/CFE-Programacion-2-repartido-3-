public class Ejercicio1 {
    public static void main(String[] args) {
        int[] array = {10, 4, 5, 7, 3, 5, 7, 9, 4, 2, 5, 7, -56};

        int pares = 0;
        int impares = 0;

        for (int num : array) {
            if (num % 2 == 0) {
                pares++;
            } else {
                impares++;
            }
        }

        System.out.println("Cantidad de pares: " + pares);
        System.out.println("Cantidad de impares: " + impares);
    }
}